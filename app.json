{
  "expo": {
    "name": "ExpenseIncome",
    "slug": "ExpenseIncome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#69AEA9",
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ruhullah.expenseincome"
    },
    "android": {
      "package": "com.ruhullah.ExpenseIncome",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 1,  // Make sure to update this for every new build
      "permissions": []  // Specify required permissions if any, or leave empty if none are needed
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1184586d-caae-4805-8c08-37f072fa1064"
      }
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "plugins": [
      "expo-font"
    ]
  }
}
